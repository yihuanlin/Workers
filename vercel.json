{
  "rewrites": [
    {
      "source": "/api/weather",
      "destination": "/weather"
    },
    {
      "source": "/api/rss",
      "destination": "/rss"
    },
    {
      "source": "/api/wallpaper",
      "destination": "/wallpaper"
    },
    {
      "source": "/api/cors",
      "destination": "/cors"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}