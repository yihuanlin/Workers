{
  "rewrites": [
    {
      "source": "/weather",
      "destination": "/api/weather"
    },
    {
      "source": "/rss",
      "destination": "/api/rss"
    },
    {
      "source": "/wallpaper",
      "destination": "/api/wallpaper"
    },
    {
      "source": "/cors",
      "destination": "/api/cors"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}