{
  "type": "module",
  "dependencies": {
    "@vercel/blob": "^0.27.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/functions": "^1.5.2",
    "@vercel/kv": "^3.0.0",
    "fast-xml-parser": "^4.5.1",
    "sharp": "^0.33.5"
  },
  "engines": {
    "node": "22.x"
  },
  "name": "workers",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:vercel",
    "build:netlify": "mkdir -p .netlify/functions && cp -r api/* .netlify/functions/ && rm .netlify/functions/wallpaper.js",
    "build:vercel": "mkdir -p public && cp -r api/* public/",
    "build:cf": "node build.js",
    "dev:netlify": "netlify dev",
    "dev:vercel": "vercel dev",
    "dev:cf": "rm -rf .wrangler && rm -rf node_modules && wrangler dev --local",
    "start": "node server.js",
    "env:vercel": "vercel env pull .env.local",
    "env:netlify": "netlify env:import .env.local",
    "env:push-vercel": "cat .env.local | grep -v '^#' | while IFS='=' read -r key value; do if [[ $key =~ ^[a-zA-Z_][a-zA-Z0-9_]*$ ]]; then vercel env add \"$key\" production <<< \"$value\"; fi; done",
    "env:push-netlify": "cat .env.local | while IFS= read -r line; do netlify env:set \"${line%%=*}\" \"${line#*=}\"; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yihuanlin/Workers.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yihuanlin/Workers/issues"
  },
  "homepage": "https://github.com/yihuanlin/Workers#readme",
  "description": ""
}